#include <iostream>

int main()
{
	int A, B, C;
	std::cin >> A >> B >> C;

	int f1,f2,f3,f4;
	f1 = (A + B) % C;
	std::cout << f1 << std::endl;
	f2 = ((A % C) + (B % C)) % C;
	std::cout << f2 << std::endl;
	f3 = (A * B) % C;
	std::cout << f3 << std::endl;
	f4 = ((A % C)*(B % C)) % C;
	std::cout << f4 << std::endl;

	return 0;
}
