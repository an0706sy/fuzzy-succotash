#include <iostream>

int main()
{
	int A;
    std::cin >> A;

	if (90 <= A)
	{
		std::cout << "A" << std::endl;
	}

	else if (80 <= A)
	{
		std::cout << "B" << std::endl;
	}

	else if (70 <= A)
	{
		std::cout << "C" << std::endl;
	}

	else if (60 <= A)
	{
		std::cout << "D" << std::endl;
	}
	else
	{
		std::cout << "F" << std::endl;
	}

	return 0;
}
